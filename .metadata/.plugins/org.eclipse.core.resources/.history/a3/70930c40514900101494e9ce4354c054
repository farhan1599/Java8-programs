package com.java8.demo;

public class Student {

	private int studentId;
	private String name;
	private int rollno;
	private String course;

	public Student(int studentId, String name, int rollno, String course) {
		this.studentId = studentId;
		this.name = name;
		this.rollno = rollno;
		this.course = course;
	}

}
